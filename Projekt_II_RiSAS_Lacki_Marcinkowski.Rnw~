% !TEX TS-program = Sweave-pdflatex
\documentclass[twoside, 12pt, a4paper]{article}

\usepackage{mystyle}
\usepackage[noae]{Sweave}



\begin{document}

	\SweaveOpts{concordance=TRUE}
	\SweaveOpts{echo=false}	


	% Here we prepare basic info
<<results=hide>>=

	library(xtable)
	library(lattice)
	library(mlmRev)

	con <- url("http://tofesi.mimuw.edu.pl/~cogito/smarterpoland/Diagnoza2011/diagnozaOsoby2011.RData")
	print(system.time({load(con)}))
	col.names  <- c("Age"
		        ,"Region"
		        ,"Income"
		        ,"Faculty"
		        ,"IsDisabled"
		        ,"SpeakEng"
		        ,"IsUnemp"
		        ,"UnempFreq"
		        ,"UnempDur")
	set <- data.frame(2011-diagnozaOsoby2011[,2071],
		          diagnozaOsoby2011[,54],  
		          diagnozaOsoby2011[,937],
		          diagnozaOsoby2011[,1525],
		          diagnozaOsoby2011[,1542],
		          diagnozaOsoby2011[,1530],
		          diagnozaOsoby2011[,2038],
		          diagnozaOsoby2011[,1582],
		          diagnozaOsoby2011[,2044])
	attr(set,"names") <- col.names

@


	% Here we generate the titlepage
		\SweaveInput{tex_Rnw/titlepage.Rnw}

	\tableofcontents

	\section{Executive Summary}
		\SweaveInput{tex_Rnw/executive_summary.Rnw}


	\section{Data Set}
		\SweaveInput{tex_Rnw/data_summary.Rnw}




\end{document}
